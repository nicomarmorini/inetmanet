# The testsuite's name, executable and where the orginial files where created
full_name:OMNeT++ TraCI Client Modules
executable:$TEXTTEST_HOME/texttest_opp_run
home_operating_system:posix

# In the sandbox, create links to the following required files and directories
link_test_path:$TEXTTEST_HOME/package.ned
link_test_path:$TEXTTEST_HOME/omnetpp.ini
link_test_path:$TEXTTEST_HOME/Car.ned
link_test_path:$TEXTTEST_HOME/Highway.ned
link_test_path:$TEXTTEST_HOME/commands.xml
link_test_path:$TEXTTEST_HOME/sumo-launchd.launch.xml
link_test_path:$TEXTTEST_HOME/sumo.sumo.cfg
link_test_path:$TEXTTEST_HOME/net.net.xml
link_test_path:$TEXTTEST_HOME/routes.rou.xml

# Where to find a run's output
log_file:logfile



[collate_file]

# The .log will be retained as "logfile"
logfile:Test_TraCI.log



[run_dependent_text]

# Ignore everything in "output" (the stdout)
output:.*
